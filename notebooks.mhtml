-accent: #5fa4ea; --accent-hover: #3b82f6; --positive: #34d399; --negative:=
 #f87171; }

body { margin: 0px; font-family: "72", sans-serif; background: var(--bg); c=
olor: var(--text-primary); }

.note { padding: 8px 16px; font-size: 12px; color: var(--text-secondary); b=
ackground: var(--surface); border-bottom: 1px solid var(--border); }

.slider-container { padding: 8px 16px; background: var(--surface); border-b=
ottom: 1px solid var(--border); }

input[type=3D"range"] { width: 100%; }

.toolbar { display: flex; align-items: center; gap: 8px; padding: 8px 16px;=
 background: var(--surface); border-bottom: 1px solid var(--border); width:=
 calc(var(--toolbar-perc) * 1%); overflow: hidden; transition: width 0.2s; =
}

.toolbar.footer { position: sticky; bottom: 0px; border-top: 1px solid var(=
--border); border-bottom: none; z-index: 5; }

.search-box { position: relative; flex: 1 1 auto; }

.search-box input { width: 100%; padding: 6px 30px 6px 8px; border-radius: =
4px; border: 1px solid var(--border); background: var(--bg); color: var(--t=
ext-primary); }

.search-box .icon { position: absolute; right: 8px; top: 50%; transform: tr=
anslateY(-50%); font-size: 14px; pointer-events: none; color: var(--text-se=
condary); }

.icon { font-family: SAP-icons; font-style: normal; font-size: 18px; line-h=
eight: 1; cursor: pointer; padding: 4px; border-radius: 4px; }

.icon:hover { background: var(--bg); }

.btn { border: none; border-radius: 4px; padding: 6px 12px; font-family: "7=
2", sans-serif; font-size: 14px; cursor: pointer; display: inline-flex; ali=
gn-items: center; justify-content: center; gap: 4px; }

.btn-reset { background: var(--accent); color: rgb(255, 255, 255); }

.btn-reset:hover { background: var(--accent-hover); }

.btn-accept { background: var(--positive); color: rgb(255, 255, 255); }

.btn-accept:hover { filter: brightness(0.9); }

.btn-reject { background: var(--negative); color: rgb(255, 255, 255); }

.btn-reject:hover { filter: brightness(0.9); }

.table-wrapper { overflow-x: auto; }

table { width: 100%; border-collapse: collapse; }

th, td { padding: 8px 12px; border-bottom: 1px solid var(--border); text-al=
ign: left; font-size: 14px; }

th { font-weight: bold; background: var(--surface); position: sticky; top: =
0px; z-index: 2; }

tr:hover td { background: rgba(0, 0, 0, 0.02); }

.product { color: var(--accent); font-weight: 600; }

.product-code { font-size: 12px; color: var(--text-secondary); }

.footer-right { margin-left: auto; display: flex; gap: 8px; }

@media (max-width: 840px) {
  .toolbar { flex-wrap: wrap; }
  .footer-right { margin-left: 0px; }
